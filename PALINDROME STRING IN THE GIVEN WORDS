def find_first_palindrome(words):
    for word in words:
        if word == word[::-1]:
            return word
    return ""
words1 = ["abc", "car", "ada", "racecar", "cool"]
words2 = ["notapalindrome", "racecar"]
output1 = find_first_palindrome(words1)
output2 = find_first_palindrome(words2)
print(output1)
print(output2)
