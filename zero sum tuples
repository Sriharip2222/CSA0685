def zero_sum_tuples(A, B, C, D):
    count = 0
    for i in A:
        for j in B:
            for k in C:
                for l in D:
                    if i + j + k + l == 0:
                        count += 1
    return count
A1 = [1, 2]
B1 = [-2, -1]
C1 = [-1, 2]
D1 = [0, 2]
print(zero_sum_tuples(A1, B1, C1, D1))

A2 = [0]
B2 = [0]
C2 = [0]
D2 = [0]
print(zero_sum_tuples(A2, B2, C2, D2))
